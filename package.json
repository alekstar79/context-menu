{
  "name": "@alekstar79/context-menu",
  "description": "Context Menu TS is a customizable radial context menu for web applications, written in TypeScript.",
  "version": "2.0.0",
  "type": "module",
  "license": "MIT",
  "author": "Aleksey Tarasenko <alekstar79@yandex.ru>",
  "main": "lib/context-menu.js",
  "module": "lib/context-menu.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/context-menu.js",
      "types": "./lib/index.d.ts"
    },
    "./vue": {
      "import": "./lib/context-menu-vue.js",
      "types": "./lib/vue.d.ts"
    },
    "./lib/styles.css": "./lib/styles.css"
  },
  "keywords": [
    "typescript",
    "radial",
    "context",
    "menu",
    "vue"
  ],
  "files": [
    "lib/",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -p tsconfig.lib.json --noEmit && vite build --config vite.lib.config.ts",
    "build:demo": "vite build --config vite.config.ts",
    "preview": "vite preview",
    "test": "vitest run",
    "test:ui": "vitest run --ui",
    "test:coverage": "vitest run --coverage"
  },
  "peerDependencies": {
    "vue": "^3.5.13"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    }
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@types/node": "^25.2.3",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "jsdom": "^28.1.0",
    "sass": "^1.75.0",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^4.0.18",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.8"
  },
  "packageManager": "yarn@3.3.0",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
